{{ tfDoNotEditStamp }}

{{range .PullConsumers -}}
module "consumer-{{hclident .Queue}}" {
  source  = "standard-ai/hedwig-queue/google"
  version = "~> {{TFGoogleQueueModuleVersion}}"

  queue          = "{{.Queue}}"

  {{with .ServiceAccount -}}
  iam_service_account = {{hclvalue .}}
  {{- end}}

  {{with .Labels -}}
  labels = {{hclvalue .}}
  {{- end}}
}

{{end}}
